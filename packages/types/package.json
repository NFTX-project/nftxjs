{
  "name": "@nftx/types",
  "version": "0.9.0",
  "description": " ",
  "homepage": "https://github.com/NFTX-project/nftxjs#readme",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/ts/index.d.ts",
  "typedoc": {
    "entryPoint": "src/index.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NFTX-project/nftxjs.git"
  },
  "scripts": {
    "lint": "eslint --fix -c ../../.eslintrc.js --ext ts,tsx src && tsc --noEmit",
    "test": "echo \"no tests\"",
    "codegen": "graphql-codegen -c nftx-v3.codegen.yml && graphql-codegen -c nftx-v3-uniswap.codegen.yml",
    "build": "rm -rf dist && yarn codegen && tsc -d --outDir dist/ts --emitDeclarationOnly && mkdir dist/cjs && echo \"module.exports = {}\" > dist/cjs/index.js",
    "prepublishOnly": "yarn build"
  },
  "bugs": {
    "url": "https://github.com/NFTX-project/nftxjs/issues"
  },
  "dependencies": {
    "@nftx/constants": "^0.9.0",
    "abitype": "^0.7.1",
    "viem": "0.1.19"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "graphql": "^16.7.1"
  }
}
