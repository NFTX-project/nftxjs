    "@nftx/subgraph": "^4.0.1",
    "@nftx/trade": "^4.0.1",
    "@nftx/types": "^4.0.0",
    "@nftx/utils": "^4.0.1",
    "@uniswap/sdk-core": "^4.0.0",
    "@uniswap/v3-sdk": "^3.10.0",
    "decimal.js": "^10.4.3",
    "jsbi": "^3.1.4",
    "viem": "^1.16.6"
